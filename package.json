{
    "name": "hello-world",
    "description": "Demo of how to build an app using marko-starter",
    "version": "1.0.0",
    "dependencies": {
        "@lasso/marko-taglib": "^1.0.15",
        "highlights": "^3.1.4",
        "language-css": "github:atom/language-css",
        "language-html": "github:atom/language-html",
        "language-javascript": "github:atom/language-javascript",
        "language-marko": "github:marko-js/atom-language-marko",
        "language-shellscript": "github:atom/language-shellscript",
        "lasso": "^3.3.1",
        "lasso-marko": "^2.4.8",
        "lasso-postcss": "^0.4.0",
        "marked": "^0.7.0",
        "marko": "^4.13.10",
        "marko-starter": "^2.0.4",
        "postcss": "^8.2.10",
        "postcss-extend-rule": "^3.0.0",
        "react": "github:orktes/atom-react",
        "tailwindcss": "^1.1.2"
    },
    "devDependencies": {
        "autoprefixer": "^9.6.1",
        "eslint": "^5.9.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.0",
        "marko-prettyprint": "^1.5.2",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.1.2",
        "prettier": "^1.15.2"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/marko-js-samples/marko-starter-demo"
    },
    "scripts": {
        "build": "rm -rf .cache/ && NODE_ENV=production marko-starter build",
        "format": "prettier src/**/*.{js,css,less} --write && marko-prettyprint src",
        "lint": "eslint src",
        "serve": "NODE_ENV=production marko-starter serve-static",
        "start": "marko-starter server",
        "test": "npm run lint"
    },
    "private": true
}
